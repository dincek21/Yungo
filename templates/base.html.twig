<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('css/app.css') }}">
        <link href="{{ asset('css/tailwind.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/jquery.dataTables.min.css') }}" rel="stylesheet">
        <link href="{{ asset('css/jquery.sweet-modal.css') }}" rel="stylesheet">
        <script src="{{ asset('js/jquery-3.6.0.js') }}"></script>
        <script src="{{ asset('js/dataTables.js') }}"></script>
        <script src="{{ asset('js/chart.js') }}"> </script>
        <script src="{{ asset('bundles/datatables/js/datatables.js') }}"></script>
        <script src="{{ asset('js/dataTables.buttons.min.js') }}"></script>
        <script src="{{ asset('js/jquery.sweet-modal.js') }}"></script>
        <script src="http://cdn.datatables.net/plug-ins/1.10.15/dataRender/datetime.js"></script>
        <script src="http://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
        {% block stylesheets %}
            {#{{ encore_entry_link_tags('app') }}#}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <div class="flex min-h-screen" >
            <nav class="w-60 flex-shrink-4" style="width:200px">
                <div class="flex-auto bg-gray-900 h-full">
                    <div class="flex flex-col overflow-y-auto" style="text-align:center;">
                        <ul class="relative m-0 p-0 list-none h-full">
                            <li class="text-white text-2xl p-4 w-full flex relative shadow-sm justify-start bg-gray-800 border-b-2 border-gray-700">
                                <img src="{{ asset('img/icon.png') }}"></img>
                            </li>
                            <li class="text-white p-4 w-full flex relative shadow-sm justify-start bg-gray-800 border-b-2 border-gray-700">
                                <div class="flex-auto my-1">
                                    <span>Admin</span>
                                </div>
                            </li>
                            <li class="p-4 w-full flex relative shadow-sm">
                                <div class="flex-auto my-1">
                                    <span class="text-white font-medium">Apartados de la cuenta</span>
                                </div>
                            </li>
                            <div class="text-blue-400 flex relative px-4 hover:bg-gray-700 cursor-pointer">
                                
                                <div class="flex-auto my-1">
                                    <a ><span>Perfil</span></a>
                                </div>
                            </div>
                            <div class="text-blue-400 flex relative px-4 hover:bg-gray-700 cursor-pointer">
                                
                                <div class="flex-auto my-1">
                                    <span><a>Configuracion</a></span>
                                </div>
                            </div>
                           
                        </ul>
                    </div>
                </div>
            </nav>
            <div class="flex flex-col w-full">
                <header class="text-white bg-gray-900 left-auto top-0 right-0 ">
                    <div class="h-5 px-6 fle relative items-center justify">
                    </div>

                    <div class="h-6 px-6 fle relative items-center justify-end">
                        <ul class="flex justify">
                            <li class="mr-3">
                                <a class="rounded-full inline-block border border-blue-400 rounded py-1 px-8 bg-blue-700 hover:bg-blue-600 text-white" href="/" title="Inicio">
                                    <svg class="h-8 w-8"  width="24" height="24" viewBox="0 0 23 26" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">  <path stroke="none" d="M0 0h24v24H0z"/>  <polyline points="5 12 3 12 12 3 21 12 19 12" />  <path d="M5 12v7a2 2 0 0 0 2 2h10a2 2 0 0 0 2 -2v-7" />  <path d="M9 21v-6a2 2 0 0 1 2 -2h2a2 2 0 0 1 2 2v6" /></svg>
                                </a>
                            </li>
                            <li class="mr-3">
                                <a class="rounded-full inline-block border border-green-400 rounded py-1 px-8 bg-green-700 hover:bg-green-600 text-white" href="/clientes" title="Clientes">
                                    <svg class="h-8 w-8"  fill="none" viewBox="0 0 23 26" stroke="currentColor">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"/></svg>
                                </a>
                            </li>
                            <li class="mr-3">
                                <a class="rounded-full inline-block border border-red-400 rounded py-1 px-8 bg-red-700 hover:bg-red-600 text-white" href="/tickets" title="Tickets">
                                    <svg class="h-8 w-8"  fill="none" viewBox="0 0 23 26" stroke="currentColor">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 5v2m0 4v2m0 4v2M5 5a2 2 0 00-2 2v3a2 2 0 110 4v3a2 2 0 002 2h14a2 2 0 002-2v-3a2 2 0 110-4V7a2 2 0 00-2-2H5z"/></svg>
                                </a>
                            </li>
                            <li class="mr-3">
                                <a class="rounded-full inline-block border border-gray-400 rounded py-1 px-8 bg-gray-700 hover:bg-gray-600 text-white" href="/inventario" title="Inventario">
                                    <svg class="h-8 w-8"  width="24" height="24" viewBox="0 0 23 26" stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">  <path stroke="none" d="M0 0h24v24H0z"/>  <path d="M14 3v4a1 1 0 0 0 1 1h4" />  <path d="M17 21h-10a2 2 0 0 1 -2 -2v-14a2 2 0 0 1 2 -2h7l5 5v11a2 2 0 0 1 -2 2z" />  <line x1="9" y1="7" x2="10" y2="7" />  <line x1="9" y1="13" x2="15" y2="13" />  <line x1="13" y1="17" x2="15" y2="17" /></svg>
                                </a>
                            </li>
                        </ul>
                    </div>
                    
                    <div class="p-5 h-6 flex relative items-center justify-end" style="top: 25px;">
                        <div class="p-0 h-10 flex relative items-center">
                            
                        </div>
                    </div>
                </header>
                {% block body %}{% endblock %}
            </div>
        </div>
    </body>
</html>
